<%= form_for get_job_search, url: {action: "search"}, html: {class: 'navbar-form navbar-right'} do |f| %>
<div class="form-group">
  <%= f.label :search %>
  <%= f.text_field :search, class: 'form-control' %>
</div>
<div class="form-group">
  <%= f.submit "Search", id: 'submit', class: 'btn btn-success' %>
</div>
<% end %>
