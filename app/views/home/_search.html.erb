<%= form_for get_job_search, url: {action: "search", :controller=>"jobs"}, html: {class: 'navbar-form navbar-right'} do |f| %>
<div class="form-group">
  <%= f.label :title %>
  <%= f.text_field :title, class: 'form-control' %>
</div>
<div class="form-group">
  <%= f.submit "Search", id: 'search', class: 'btn btn-success' %>
</div>
<% end %>
