<span class="error_message"><%= flash[:alert] %></span>
<%= form_for @category do |f| %>
<div class="form-group">
  <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control' %>
</div>
<div class="form-group">
  <%= f.submit "Send", id: 'submit', class: 'btn btn-primary' %>
</div>
<% end %>
