<%= form_for get_job_search, url: search_jobs_path(get_job_search), html: {class: 'navbar-form navbar-right'} do |f| %>
<div class="form-group">
  <%= f.label :title, class: 'hidden' %>
  <%= f.text_field :title, name: 'job[search]', class: 'form-control' %>
</div>
<div class="form-group">
  <%= f.submit "Search", id: 'search', class: 'btn btn-success' %>
</div>
<% end %>
